<interface>
	<name>Get Arris SB8200 Up Stats</name>
	<description>Queries the upstream stats</description>
	<script_path>perl |path_cacti|/scripts/query_sb8200_stats.pl</script_path>
        <arg_prepend>|host_hostname| Up</arg_prepend>
	<arg_index>index</arg_index>
	<arg_query>query</arg_query>
	<arg_get>get</arg_get>
	<arg_num_indexes>num_indexes</arg_num_indexes>
	<output_delimeter>:</output_delimeter>
	<index_order>Chan</index_order>
	<index_order_type>alphabetic</index_order_type>
	<index_title_format>|chosen_order_field|</index_title_format>

	<fields>
		<Chan>
			<name>Channel</name>
			<direction>input</direction>
			<query_name>Chan</query_name>
		</Chan>
		<ChanId>
			<name>Channel ID</name>
			<direction>input</direction>
			<query_name>ChanId</query_name>
		</ChanId>
		<Status>
			<name>Status</name>
			<direction>input</direction>
			<query_name>Stat</query_name>
		</Status>
		<Type>
			<name>Type</name>
			<direction>input</direction>
			<query_name>Type</query_name>
		</Type>

		<Freq>
			<name>Frequency</name>
			<direction>output</direction>
			<query_name>Freq</query_name>
		</Freq>
		<Width>
			<name>Width</name>
			<direction>output</direction>
			<query_name>Width</query_name>
		</Width>
		<Power>
			<name>Power</name>
			<direction>output</direction>
			<query_name>Power</query_name>
		</Power>
	</fields>
</interface>
